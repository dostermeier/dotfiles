export PATH=$HOME/bin:/usr/local/bin:/opt/local/bin:/opt/local/sbin:$PATH
export MANPATH=/usr/local/man:/opt/local/share/man:$MANPATH

export GIT_TREE=/Volumes/git_tree

# Set default editer to sublime text
export EDITOR='subl -w'

